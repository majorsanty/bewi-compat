{
  "schemaVersion": 1,
  "id": "bewi-compat",
  "version": "${version}",

  "name": "Bottled Bewitchment",
  "description": "Compatibility mod focused around Bewitchment compat with some guis",
  "authors": [
    "Santy"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/bewitchment-thirst-compatibility",
    "sources": "https://github.com/majorsanty/bewi-compat"
  },

  "license": "MIT",
  "icon": "assets/bewicompat/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.bewicompat.BewitchmentCompatUtils"
    ]
  },
  "mixins": [
    "bewicompat.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "parent": "bewitchment"
    }
  },
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "bewitchment": ">=1.19-4",
    "dehydration": ">=1.3",
    "environmentz": ">=1.3.3"
  },
  "suggests": {
    "besmirchment": ">=1.19-5"
  }
}
